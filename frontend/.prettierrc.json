{
    "$schema": "https://json.schemastore.org/prettierrc",
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "tabWidth": 4,
    "printWidth": 100,
    "arrowParens": "always",
    "endOfLine": "lf",
    "vueIndentScriptAndStyle": true,
    "singleAttributePerLine": true,
    "jsxSingleQuote": false
}
